<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>lab12-goods-management</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>jdbc.driverClassName</param-name>
    <param-value>org.h2.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>jdbc.url</param-name>
    <param-value>jdbc:h2:tcp://localhost/~/test</param-value>
  </context-param>
  <context-param>
    <param-name>jdbc.username</param-name>
    <param-value>sa</param-value>
  </context-param>
  <context-param>
    <param-name>jdbc.password</param-name>
    <param-value></param-value>
  </context-param>
  <resource-ref>
    <res-ref-name>jdbc/h2_db</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <context-param>
    <param-name>dbcp_resource_name</param-name>
    <param-value>jdbc/h2_db</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>BooksListController</display-name>
    <servlet-name>BooksListController</servlet-name>
    <servlet-class>com.wplab.goodsmgr.controller.BooksListController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BooksListController</servlet-name>
    <url-pattern>/books/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BooksRegisterFormController</display-name>
    <servlet-name>BooksRegisterFormController</servlet-name>
    <servlet-class>com.wplab.goodsmgr.controller.BooksRegisterFormController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BooksRegisterFormController</servlet-name>
    <url-pattern>/books/register_form</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BooksRegisterController</display-name>
    <servlet-name>BooksRegisterController</servlet-name>
    <servlet-class>com.wplab.goodsmgr.controller.BooksRegisterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BooksRegisterController</servlet-name>
    <url-pattern>/books/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BooksUpdateFormController</display-name>
    <servlet-name>BooksUpdateFormController</servlet-name>
    <servlet-class>com.wplab.goodsmgr.controller.BooksUpdateFormController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BooksUpdateFormController</servlet-name>
    <url-pattern>/books/update_form</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BooksUpdateController</display-name>
    <servlet-name>BooksUpdateController</servlet-name>
    <servlet-class>com.wplab.goodsmgr.controller.BooksUpdateController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BooksUpdateController</servlet-name>
    <url-pattern>/books/update</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BooksDeleteController</display-name>
    <servlet-name>BooksDeleteController</servlet-name>
    <servlet-class>com.wplab.goodsmgr.controller.BooksDeleteController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BooksDeleteController</servlet-name>
    <url-pattern>/books/delete</url-pattern>
  </servlet-mapping>
</web-app>